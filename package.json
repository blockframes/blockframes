{
  "name": "blockframes",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./main.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "ng": "nx",
    "start": "nx serve --hmr --disable-host-check",
    "build:stats": "nx build --prod --stats-json",
    "build:catalog": "cross-env nx build catalog --configuration=\"$ENV\"",
    "build:festival": "cross-env nx build festival --configuration=\"$ENV\"",
    "build:financiers": "cross-env nx build financiers --configuration=\"$ENV\"",
    "build:backend-functions": "cross-env nx build backend-functions --configuration=\"$ENV\"",
    "build:backend-ops": "cross-env nx build backend-ops --configuration=\"$ENV\"",
    "build:crm": "cross-env nx build crm --configuration=\"$ENV\"",
    "build:all": "npm run build:backend-functions && npm run build:backend-ops && npm run build:catalog && npm run build:festival && npm run build:crm",
    "build:all:concurrently": "concurrently 'npm:build:backend-functions' 'npm:build:backend-ops' 'npm:build:catalog' 'npm:build:festival' 'npm:build:crm'",
    "deploy:functions": "cross-env nx deploy backend-functions --configuration=$ENV",
    "deploy:catalog": "cross-env nx deploy catalog --configuration=$ENV",
    "deploy:festival": "cross-env nx deploy festival --configuration=$ENV",
    "deploy:crm": "cross-env nx deploy crm --configuration=$ENV",
    "deploy:firestore": "cross-env firebase deploy --only firestore --force --project=$ENV",
    "deploy:firestore:rules": "cross-env firebase deploy --only firestore:rules --project=$ENV",
    "deploy:storage": "cross-env firebase deploy --only storage --project=$ENV",
    "deploy:functions:config": "ts-node -P tools/tsconfig.tools.json tools/scripts/deploy-function-config.ts",
    "firebase:serve": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase serve --only functions\"",
    "firebase:shell": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase functions:shell\" --raw",
    "firebase:emulator": "firebase functions:config:get > .runtimeconfig.json && firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "test": "nx test",
    "test:testing:firebase": "firebase functions:config:get > .runtimeconfig.json && firebase emulators:exec 'nx test testing-firebase' ",
    "lint": "nx lint",
    "e2e": "npm run e2e:catalog && npm run e2e:festival",
    "pree2e": "cross-env nx build backend-ops --configuration=\"$ENV\" && node dist/apps/backend-ops/main.js",
    "e2e:catalog": "cross-env nx e2e catalog-e2e --configuration=\"$ENV\" --headless",
    "e2e:festival": "cross-env nx e2e festival-e2e --configuration=\"$ENV\" --headless",
    "affected:apps": "nx affected:apps --base=origin/master",
    "affected:libs": "nx affected:libs --base=origin/master",
    "affected:build": "nx affected:build --base=origin/master",
    "affected:e2e": "nx affected:e2e --base=origin/master",
    "affected:test": "firebase emulators:exec 'nx affected:test --base=origin/develop'",
    "affected:lint": "nx affected:lint --base=origin/master",
    "affected:dep-graph": "nx affected:dep-graph --base=origin/master",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "migrate:deploy-secrets": "ts-node -P tools/tsconfig.tools.json tools/scripts/migrate-bash-secrets.ts",
    "env:detect-service-account": "ts-node -P tools/tsconfig.tools.json tools/scripts/check-google-application-credentials.ts",
    "update": "nx update @nrwl/workspace",
    "update:check": "nx update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "contracts:build": "waffle ./waffle-config.json",
    "affected": "nx affected",
    "analyze:marketplace": "webpack-bundle-analyzer dist/apps/catalog/marketplace/stats.json --port=8000",
    "pwm": "pwmetrics --config",
    "bundlesize": "bundlesize",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js",
    "prerender:festival": "nx build festival --prod && nx run festival:server --configuration production && angular-prerender --browser-target festival:build --server-target festival:server",
    "db:download-prod": "ts-node -P tools/tsconfig.tools.json tools/scripts/get-prod-backup.ts",
    "db:anonymize": "ts-node -P tools/tsconfig.tools.json tools/scripts/anonymize-db.ts",
    "db:upload-ci": "ts-node -P tools/tsconfig.tools.json tools/scripts/upload-db.ts",
    "db:fetch-from-ci": "ts-node -P tools/tsconfig.tools.json tools/scripts/fetch-db-from-ci.ts",
    "backend-ops": "ts-node -P tools/tsconfig.tools.json apps/backend-ops/src/main.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.4",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "^10.0.4",
