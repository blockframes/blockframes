    "@sendgrid/mail": "^6.4.0",
    "@sentry/browser": "^5.5.0",
    "@sentry/node": "^5.5.0",
    "@types/object-hash": "^1.3.0",
    "@types/sendgrid": "^4.3.0",
    "akita-ng-fire": "^2.0.0-alpha.20",
    "algoliasearch": "^3.33.0",
    "core-js": "^3.3.2",
    "ethereum-blockies-base64": "^1.0.2",
    "express": "4.17.1",
