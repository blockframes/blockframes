{
  "name": "blockframes",
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/apps/backend-functions/main.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "ng": "ng",
    "start": "npx ng serve --hmr --disable-host-check",
    "build:delivery": "npx ng build delivery --prod --base-href /delivery/",
    "build:movie-financing": "npx ng build movie-financing --prod --base-href /movie-financing/",
    "build:ip-upload": "npx ng build ip-upload --prod --base-href /ip-upload/",
    "build:root": "cp apps/index.html dist/apps/ && rm -rf dist/apps/assets && cp -R libs/shared-assets dist/apps/assets",
    "build:all": "npm run build:delivery && npm run build:movie-financing && npm run build:ip-upload && npm run build:root",
    "build:functions": "npx ng build backend-functions",
    "firebase:serve": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase serve --only functions\"",
    "firebase:shell": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase functions:shell\" --raw",
    "firebase:deploy": "firebase deploy",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "./node_modules/.bin/ng e2e ip-upload-e2e && ./node_modules/.bin/ng e2e movie-financing-e2e && ./node_modules/.bin/ng e2e delivery-e2e",
    "e2e:dev": "npx ng e2e --base-url http://localhost:4200 --watch",
    "e2e:dev:movie-financing": "npx ng e2e movie-financing-e2e --base-url http://localhost:4200 --watch",
    "e2e:dev:delivery": "npx ng e2e delivery-e2e --base-url http://localhost:4200 --watch",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "contracts:build": "npx waffle ./waffle-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/fire": "^5.1.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/pwa": "^0.13.1",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@datorama/akita": "^3.8.3",
    "@fullpage/angular-fullpage": "0.0.11",
    "@google-cloud/pubsub": "^0.24.1",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/router-store": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@nrwl/nx": "7.8.6",
    "core-js": "^2.6.4",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^4.0.27",
    "express": "4.16.4",
    "firebase": "^5.8.2",
    "firebase-admin": "^7.4.0",
    "firebase-functions": "^2.3.1",
    "hammerjs": "^2.0.8",
    "ngx-mat-select-search": "^1.7.2",
    "node-gyp": "^4.0.0",
    "npm": "^6.9.0",
    "punycode": "^2.1.1",
    "rxjs": "~6.5.2",
    "zone.js": "^0.9.1",
    "@google-cloud/storage": "^2.4.2",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.1",
    "@angular-devkit/build-ng-packagr": "~0.800.1",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@datorama/akita-ng-router-store": "^3.0.2",
    "@datorama/akita-ngdevtools": "^1.0.4",
    "@nrwl/builders": "8.0.2",
    "@nrwl/schematics": "8.0.2",
    "@types/express": "4.16.1",
    "@types/jest": "^23.3.14",
    "@types/jquery": "3.3.29",
    "@types/line-reader": "0.0.28",
    "@types/lodash": "^4.14.123",
    "@types/node": "~10.12.23",
    "@types/tmp": "0.0.34",
    "akita-schematics": "^2.0.0",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "cypress": "^3.1.5",
    "ethereum-waffle": "^2.0.9",
    "jest": "^23.6.0",
    "jest-preset-angular": "6.0.2",
    "ng-packagr": "^5.2.0",
    "openzeppelin-solidity": "^2.2.0",
    "pdfkit": "^0.9.0",
    "prettier": "1.16.4",
    "tmp": "0.0.33",
    "ts-node": "~8.0.2",
    "tsickle": "0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.12.1",
    "typescript": "^3.4.1"
  }
}
