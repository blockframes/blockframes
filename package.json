    "express": "^4.17.1",
    "firebase": "^7.16.1",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.8.0",
    "fs-extra": "^8.1.0",
    "generate-password": "^1.4.2",
    "handlebars": "^4.5.3",
    "intersection-observer": "^0.11.0",
    "jwplatform": "0.0.5",
    "leaflet": "^1.6.0",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.20",
    "ng-apexcharts": "^1.2.0",
