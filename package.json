    "ng-form-factory": "0.0.18",
    "ng-intercom": "^8.0.0",
    "ngx-image-cropper": "^3.0.3",
    "puppeteer": "^12.0.1",
    "rxjs": "~6.5.5",
    "twilio": "3.67.1",
    "twilio-video": "^2.16.0",
    "xlsx": "^0.17.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
