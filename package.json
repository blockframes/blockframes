{
  "name": "blockframes",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./main.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "ng": "ng",
    "start": "npx ng serve --hmr --disable-host-check",
    "build:stats": "ng build --prod --stats-json",
    "build:catalog": "npx ng build catalog --configuration=\"${ENV:-dev}\"",
    "build:festival": "npx ng build festival --base-href /festival/ --configuration=\"${ENV:-dev}\"",
    "build:backend-functions": "npx ng build backend-functions --configuration=\"${ENV:-dev}\"",
    "build:backend-ops": "npx ng build backend-ops --configuration=\"${ENV:-dev}\"",
    "build:all": "npm run build:backend-functions && npm run build:backend-ops && npm run build:catalog && npm run build:festival",
    "firebase:serve": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase serve --only functions\"",
    "firebase:shell": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase functions:shell\" --raw",
    "firebase:emulator": "firebase emulators:start --only firestore",
    "firebase:deploy": "firebase deploy",
    "test": "jest --maxWorkers=2",
    "lint": "npx ng lint",
    "e2e": "npm run e2e:catalog && npm run e2e:festival",
    "pree2e": "ng build backend-ops --configuration=\"${ENV}\" && node dist/apps/backend-ops/main.js",
    "e2e:catalog": "./node_modules/.bin/ng e2e catalog-e2e --configuration=\"${ENV}\" --headless",
    "affected:apps": "./node_modules/.bin/nx affected:apps --base=origin/master",
    "affected:libs": "./node_modules/.bin/nx affected:libs --base=origin/master",
    "affected:build": "./node_modules/.bin/nx affected:build --base=origin/master",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e --base=origin/master",
    "affected:test": "./node_modules/.bin/nx affected:test --base=origin/master",
    "affected:lint": "./node_modules/.bin/nx affected:lint --base=origin/master",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph --base=origin/master",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "contracts:build": "npx waffle ./waffle-config.json",
    "affected": "./node_modules/.bin/nx affected",
    "analyze:marketplace": "webpack-bundle-analyzer dist/apps/catalog/marketplace/stats.json --port=8000",
    "pwm": "pwmetrics --config",
    "bundlesize": "bundlesize",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "storybook:ui": "npx ng run ui:storybook",
    "storybook:movie": "npx ng run movie:storybook",
    "prerender:festival": "npx ng build festival --prod && npx ng run festival:server --configuration production && npx angular-prerender --browser-target festival:build --server-target festival:server"
  },
  "private": true,
