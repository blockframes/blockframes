    "husky": "^3.0.5",
    "jest": "24.9.0",
    "jest-environment-jsdom-thirteen": "^1.0.1",
    "jest-preset-angular": "^8.0.0",
    "ng-packagr": "^5.2.0",
    "openzeppelin-solidity": "^2.2.0",
    "pdfkit": "^0.10.0",
    "prettier": "1.18.2",
    "pwmetrics": "^4.2.2",
    "ts-jest": "^24.2.0",
    "ts-node": "~8.4.1",
