  },
  "pwmetrics": {
    "url": "http://localhost:4200/"
  },
  "bundlesize": [
    {
      "path": "./dist/apps/catalog/main**.js",
      "maxSize": "7 MB",
      "compression": "none"
    },
    {
      "path": "./dist/apps/backend-functions/main**.js",
      "maxSize": "1 MB",
      "compression": "none"
    }
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "node tools/scripts/pre-commit.js"
    }
  }
}

