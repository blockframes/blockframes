  "pwmetrics": {
    "url": "http://localhost:4200/"
  },
  "bundlesize": [
    {
      "path": "./dist/apps/main**.js",
      "maxSize": "4.5 MB",
      "compression": "none"
    },
    {
      "path": "./dist/apps/delivery/main**.js",
      "maxSize": "6 MB",
