    "algoliasearch": "^4.8.4",
    "angular-calendar": "^0.28.5",
    "angular-idle-preload": "^3.0.0",
    "apexcharts": "^3.15.3",
    "core-js": "^3.3.2",
    "date-fns": "^2.11.0",
    "dependency-graph": "^0.11.0",
    "firebase": "^8.2.2",
    "firebase-admin": "^9.4.1",
