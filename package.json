    "node": "^16.14.0",
    "npm": "^8.5.0"
  },
  "scripts": {
    "ng": "nx",
    "start": "ts-node -P tools/tsconfig.tools.json apps/backend-ops/src/main.ts",
    "build:catalog": "cross-env nx build catalog --configuration=$NG_CONFIG",
    "build:festival": "cross-env nx build festival --configuration=$NG_CONFIG",
    "build:financiers": "cross-env nx build financiers --configuration=$NG_CONFIG",
    "build:backend-functions": "cross-env nx build backend-functions --configuration=$NG_CONFIG",
    "build:backend-ops": "cross-env nx build backend-ops --configuration=$NG_CONFIG --maxWorkers=8",
    "build:crm": "cross-env nx build crm --configuration=$NG_CONFIG",
    "build:cms": "cross-env nx build cms --configuration=$NG_CONFIG",
    "build:rules": "",
    "build:all": "cross-env nx run-many --target=build --all --configuration=$NG_CONFIG",
    "deploy:functions": "cross-env nx deploy backend-functions --configuration=$NG_CONFIG",
    "deploy:catalog": "cross-env nx deploy catalog --configuration=$NG_CONFIG",
    "deploy:festival": "cross-env nx deploy festival --configuration=$NG_CONFIG",
    "deploy:crm": "cross-env nx deploy crm --configuration=$NG_CONFIG",
    "deploy:cms": "cross-env nx deploy cms --configuration=$NG_CONFIG",
    "deploy:firestore": "cross-env firebase deploy --only firestore --force --project=$PROJECT_ID",
    "deploy:firestore:rules": "cross-env firebase deploy --only firestore:rules --project=$PROJECT_ID",
    "deploy:storage": "cross-env firebase deploy --only storage --project=$PROJECT_ID",
    "deploy:functions:config": "ts-node -P tools/tsconfig.tools.json tools/scripts/deploy-function-config.ts",
    "functions:shell": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase functions:shell\" --raw",
    "emulators:start": "npm start emulators",
    "test": "npm start emulatorsUnitTests 'cross-env nx run-many --target=test --all --configuration=$NG_CONFIG'",
    "lint": "cross-env nx run-many --target=lint --all --configuration=$NG_CONFIG",
    "lint:backend-functions": "cross-env nx run backend-functions:lint --configuration=$NG_CONFIG",
    "e2e": "cross-env nx run-many --target=e2e --all  --configuration=$NG_CONFIG",
    "e2e:catalog": "cross-env nx e2e catalog-e2e --configuration=$NG_CONFIG --headless",
    "e2e:catalog:incognito": "cross-env nx e2e catalog-e2e --configuration=emulator-incognito",
    "e2e:catalog:incognito:watch": "cross-env nx e2e catalog-e2e --configuration=emulator-incognito --watch",
    "e2e:festival": "cross-env nx e2e festival-e2e --configuration=$NG_CONFIG --headless",
    "e2e:festival:incognito": "cross-env nx e2e festival-e2e --configuration=emulator-incognito",
    "e2e:festival:incognito:watch": "cross-env nx e2e festival-e2e --configuration=emulator-incognito --watch",
    "affected:apps": "cross-env nx affected:apps --configuration=$NG_CONFIG",
    "affected:libs": "cross-env nx affected:libs --configuration=$NG_CONFIG",
    "affected:build": "cross-env nx affected:build --configuration=$NG_CONFIG",
    "affected:e2e": "cross-env nx affected:e2e --configuration=$NG_CONFIG",
    "affected:lint": "cross-env nx affected:lint --configuration=$NG_CONFIG",
    "affected:test": "cross-env nx affected:test --base=origin/develop --configuration=$NG_CONFIG",
    "affected:dep-graph": "nx affected:dep-graph",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "affected": "nx affected",
    "pwm": "pwmetrics --config",
    "postinstall": "ngcc --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "prerender:festival": "nx build festival --prod && nx run festival:server --configuration production && angular-prerender --browser-target festival:build --server-target festival:server",
    "backend-ops": "ts-node -P tools/tsconfig.tools.json apps/backend-ops/src/main.ts"
  },
