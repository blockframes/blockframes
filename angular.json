    "delivery": {
      "root": "apps/delivery/delivery/",
      "sourceRoot": "apps/delivery/delivery/src",
      "projectType": "application",
      "prefix": "delivery",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/delivery",
            "index": "apps/delivery/delivery/src/index.html",
            "main": "apps/delivery/delivery/src/main.ts",
            "polyfills": "apps/delivery/delivery/src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["styles"]
            },
            "tsConfig": "apps/delivery/delivery/tsconfig.app.json",
            "assets": [
              "apps/delivery/delivery/src/favicon.ico",
              "apps/delivery/delivery/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/ui/src/lib/assets",
                "output": "./assets"
              }
            ],
            "styles": ["apps/delivery/delivery/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "dev": {},
            "production": {
              "fileReplacements": [
                {
                  "replace": "env/env.ts",
                  "with": "env/env.prod.ts"
                },
                {
                  "replace": "apps/delivery/delivery/src/environments/environment.ts",
                  "with": "apps/delivery/delivery/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "serviceWorker": true
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "env/env.ts",
                  "with": "env/env.staging.ts"
                },
                {
                  "replace": "apps/delivery/delivery/src/environments/environment.ts",
                  "with": "apps/delivery/delivery/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "serviceWorker": true
            },
            "ci": {
              "fileReplacements": [
                {
                  "replace": "env/env.ts",
                  "with": "env/env.ci.ts"
                },
                {
                  "replace": "apps/delivery/delivery/src/environments/environment.ts",
                  "with": "apps/delivery/delivery/src/environments/environment.ci.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "delivery:build"
          },
          "configurations": {
            "dev": {},
            "production": {
              "browserTarget": "delivery:build:production"
            },
            "staging": {
              "browserTarget": "delivery:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "delivery:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/delivery/delivery/tsconfig.app.json",
              "apps/delivery/delivery/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/delivery/delivery/jest.config.js",
            "tsConfig": "apps/delivery/delivery/tsconfig.spec.json",
            "setupFile": "apps/delivery/delivery/src/test-setup.ts",
            "passWithNoTests": true
