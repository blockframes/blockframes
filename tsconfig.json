{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "esNext",
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "es2019", "dom"],
    "baseUrl": ".",
    "paths": {
      "@env": ["env/env.ts"],
      "@blockframes/auth": ["libs/user/src/lib/auth/index.ts"],
      "@blockframes/auth/*": ["libs/user/src/lib/auth/*"],
      "@blockframes/auth/types": ["libs/user/src/lib/auth/+state/auth.firestore.ts"],
      "@blockframes/ui": ["libs/ui/src/index.ts"],
      "@blockframes/ui/*": ["libs/ui/src/lib/*"],
      "@blockframes/utils": ["libs/utils/src/index.ts"],
      "@blockframes/utils/*": ["libs/utils/src/lib/*"],
      "@blockframes/utils/spreadsheet": ["libs/utils/src/lib/spreadsheet/index.ts"],
      "@blockframes/movie": ["libs/movie/src/index.ts"],
      "@blockframes/movie/movie/*": ["libs/movie/src/lib/movie/*"],
      "@blockframes/movie/import": [
        "libs/movie/src/lib/movie/components/import/import-movie.module.ts"
      ],
      "@blockframes/movie/types": ["libs/movie/src/lib/movie/+state/movie.firestore.ts"],
      "@blockframes/movie/distribution-deals": ["libs/movie/src/lib/distribution-deals/index.ts"],
      "@blockframes/movie/distribution-deals/*": ["libs/movie/src/lib/distribution-deals/*"],
      "@blockframes/ethers": ["libs/ethers/src/index.ts"],
      "@blockframes/ethers/*": ["libs/ethers/src/lib/*"],
      "@blockframes/smart-contracts/*": ["smart-contracts/build/*"],
      "@blockframes/smart-contracts/quorum/*": ["smart-contracts/quorum/*"],
      "@blockframes/organization": ["libs/organization/src/index.ts"],
      "@blockframes/organization/*": ["libs/organization/src/lib/*"],
      "@blockframes/organization/types": [
        "libs/organization/src/lib/+state/organization.firestore.ts"
      ],
      "@blockframes/permissions/types": [
        "libs/organization/src/lib/permissions/+state/permissions.firestore.ts"
      ],
      "@blockframes/invitation/types": [
        "libs/notification/src/lib/invitation/+state/invitation.firestore.ts"
      ],
      "@blockframes/notification/types": [
        "libs/notification/src/lib/notification/+state/notification.firestore.ts"
      ],
      "@blockframes/notification": ["libs/notification/src/index.ts"],
      "@blockframes/notification/*": ["libs/notification/src/lib/*"],
      "@blockframes/material/material/types": [
        "libs/material/src/lib/material/+state/material.firestore.ts"
      ],
      "@blockframes/material/delivery/types": [
        "libs/material/src/lib/delivery/+state/delivery.firestore.ts"
      ],
      "@blockframes/material/template/types": [
        "libs/material/src/lib/template/+state/template.firestore.ts"
      ],
      "@blockframes/material/delivery/stakeholder/types": [
        "libs/material/src/lib/delivery/stakeholder/+state/stakeholder.firestore.ts"
      ],
      "@blockframes/material": ["libs/material/src/index.ts"],
      "@blockframes/template": ["libs/material/src/index.ts"],
      "@blockframes/contract": ["libs/contract/src/index.ts"],
      "@blockframes/contract/*": ["libs/contract/src/lib/*"],
      "@blockframes/financial-report": ["libs/financial-report/src/index.ts"],
      "@blockframes/financial-report/*": ["libs/financial-report/src/lib/*"],
      "@blockframes/admin": ["libs/admin/src/index.ts"],
      "@blockframes/admin/*": ["libs/admin/src/lib/*"],
      "@blockframes/user": ["libs/user/src/lib/user/index.ts"],
      "@blockframes/user/*": ["libs/user/src/lib/user/*"],
      "@blockframes/apps/delivery/*": ["apps/delivery/delivery/src/app/*"],
      "@blockframes/apps/catalog/marketplace/*": ["apps/catalog/src/app/marketplace/*"],
      "@blockframes/apps/catalog/dashboard/*": ["apps/catalog/src/app/dashboard/*"],
      "@blockframes/apps/catalog/*": ["apps/catalog/catalog/src/app/*"],
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true,
    "strictTemplates": false
  },
  "exclude": ["node_modules", "tmp"]
}
